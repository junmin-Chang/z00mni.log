{
  "name": "@zeplin/extension-model",
  "description": "Models exposed to Zeplin extensions",
  "homepage": "https://zeplin.io",
  "version": "2.11.1",
  "keywords": [
    "zeplin",
    "extension",
    "model"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist zeplin-extension-model.zip",
    "lint": "node_modules/.bin/eslint -c ./.eslintrc.js ./src",
    "docgen": "./node_modules/.bin/jsdoc ./src/*.js -d ./docs",
    "prebuild": "npm run clean && npm run lint",
    "build": "node_modules/.bin/webpack",
    "prepublish": "NODE_ENV=prod npm run build",
    "deploy": "./deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zeplin/extension-model.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "eslint": "^4.10.0",
    "jsdoc": "^3.5.5",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.8.1"
  }
}
